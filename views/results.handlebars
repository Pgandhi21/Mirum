<div class="results">
  {{#each data}}
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
          {{title}}
        <a href="/post/{{id}}">{{title}}</a>
      </h2>
    </div>
    <div class="card-body">
      <p>{{summary}}</p>>
      <a href="{{link}}">{{link}}</a>
      <p>{{date}}</p>
    </div>
  </article>
  {{/each}}
</div>